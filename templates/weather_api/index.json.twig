{
    "city": "Police",
    "country": "PL",
    "measurements": [
        {% for measurement in measurements %}
        {
            "date": "{{ measurement.date | date('Y-m-d') }}",
            "celsius": "{{ measurement.temperature }}",
            "cloudy": "{{ measurement.cloudy }}",
            "humidity": "{{ measurement.humidity }}",
            "ChanceOfPrecipitation": "{{ measurement.ChanceOfPrecipitation }}",
            "Fahrehneit": "{{ measurement.Fahrehneit }}"

        }{% if not loop.last %},{% endif %}
        {% endfor %}
    ]
}